.short-bio {
  position: relative
  background-color: $darkest-grey
  color: #f7f7f7
  display: inline-block
  font-family: 'Source Code Pro', monospace;
  line-height: 1.5
  margin-top: 200px
  padding: 5px 15px
}

.cta{
  display: block
  background-color: #9977bd
  color: #f7f7f7
  display: inline-block
  font-family: 'Source Code Pro', monospace;
  line-height: 1.5
  padding: 5px 15px
}

.interface {
  height: 96vh
  padding-top: 70px;
  width: 100%
}

.home-eyes {
  position: absolute
}

.home-sketch {
  position: absolute
  left: 465px;
}

.portfolio-img {
  position: absolute
  right: 70px
  margin: 70px 20px 0 0
  padding: 0 10px 30px 10px
}

.second-layer {
  margin-top: 600px
}

.example-enter {
  opacity: 0.01;
}

.example-enter.example-enter-active {
  opacity: 1;
  transition: opacity 500ms ease-in;
}

.example-leave {
  opacity: 1;
}

.example-leave.example-leave-active {
  opacity: 0.01;
  transition: opacity 300ms ease-in;
}
