{
  "name": "katryn_mcintosh",
  "version": "1.0.0",
  "homepage": "https://katrynmc.github.io",
  "description": "A portfolio site of art and a front end development showpiece.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "lint": "eslint --ext .ts --ext .tsx src/",
    "pretty": "prettier --write 'src{/**/*.ts,/**/*.tsx}'",
    "start-dev": "NODE_ENV=development webpack-dev-server --hot --mode development",
    "start": "NODE_ENV=production node app.js",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "react",
    "design",
    "webpack"
  ],
  "author": "Katryn McIntosh",
  "license": "ISC",
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^5.0.0",
    "prettier": "^3.0.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.14",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.2.0",
    "webpack": "^5.88.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "volta": {
    "node": "18.16.1",
    "yarn": "1.22.19"
  }
}
